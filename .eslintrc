{
  "parser": "babel-eslint",

  "env": {
    "es6": true,
  },

  "plugins": [
    "react",
    "react-native"
  ],

  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
  },

  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
  ],

  "rules": {
    "comma-dangle": [2, "always-multiline"],
    "react/prop-types": [0]
  }
}